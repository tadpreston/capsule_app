<div class="row">
	<div class="col-md-12">
		<div class="section-head wow fadeIn animated animated" data-wow-offset="10" data-wow-duration="1.5s" style="visibility: visible; -webkit-animation-duration: 1.5s;">
			<h1>Reset Your Password</h1>
			<p>Please wait while we process your request...</p>
			<span class="underline"></span>
			<div class="row" id="status_wrapper">
				<div id="success" style="display: none;"><br />
					<i class="fa fa-check fa-2x"></i><br />
					<h2>Success!</h2>
					<h3>Your password has been reset.</h3>
				</div>
				<div id="error" style="display: none;"><br />
					<i class="fa fa-times fa-2x"></i><br />
					<h2>Uh oh.</h2>
					<h3>We ran into an error while processing your request.</h3>
				</div>
				<div id="processing" style="display: none;"><br />
					<i class="fa fa-circle-o-notch fa-spin fa-2x"></i><br />
					<h2>Processing...</h2>
				</div>
				<div id="no-token" style="display: none;"><br />
					<i class="fa fa-times fa-2x"></i><br />
					<h2>Uh oh.</h2>
					<h3>You may have gotten here by mistake.</h3><br />
					<a href="/" class="btn btn-primary" style="background: purple;">Go to the Home Page</a>
				</div>
				<div id="password-form" style="display: none;">
					<div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12">
					<form>
						<div class="form-group">
							<div class="row text-center">
							<label>Password</label>
							</div>
							<div style="max-width: 80%; margin: 0 auto;">
							<div class="row">
							<input type="password" id="password" class="form-control text-center" placeholder="Enter a minimum of 6 characters" />
							<span
								id="passwordValidationMessage"
								class="validationMessage"></span>
							</div>
							</div>
						</div>
						<div class="form-group">
							<div class="row text-center">
							<label>Confirm Password</label>
							</div>
							<div style="max-width: 80%; margin: 0 auto;">
							<div class="row">
							<input type="password" id="confirm-password" class="form-control text-center" />
							</div>
							</div>
						</div>
						<div class="form-group">
							<button
								id="btn-do-reset"
								class="btn btn-primary"
								style="background: purple">
								Reset Password
							</button>
						</div>
					</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

<% content_for :scripts do %>
	<script>
		$(document).ready(function() {
			var handler = new PasswordResetHandler('<%= @token %>');
			handler.initialize();
		});
	</script>
<% end %>
