%tr
  %td= admin_user.id
  %td= admin_user.first_name
  %td= admin_user.last_name
  %td= admin_user.email
  %td
    = link_to 'Show', admin_admin_user_path(admin_user)
    |
    = link_to 'Edit', edit_admin_admin_user_path(admin_user)
    |
    = link_to 'Delete', admin_admin_user_path(admin_user), method: :delete, data: { confirm: 'Are you sure you want to delete this user?' }
