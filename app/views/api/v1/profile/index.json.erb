{
  "status":"Success",
  "response": {
    "alert": {
      "text": "Test alert text body",
      "title": "Test alert title",
      "action_url": "capsule://actionUrl",
      "action_title": "Take Action",
      "cancel_url": "capsule://cancelUrl",
      "cancel_title": "Cancel"
    },
    "byme": [ <%= @capsules.html_safe %> ],
    "watched": {
      "capsules": [ <%= @watched_capsules.html_safe %> ],
      "locations": [ <%= @watched_locations.html_safe %> ],
      "users": [
        <%= @following.collect { |user| render(partial: 'api/v1/users/array_user', locals: { user: user }) }.join(',').html_safe %>
      ]
    },
    "followers": [
      <%= @followers.collect { |user|  render(partial: 'api/v1/users/array_user', locals: { user: user }) }.join(',').html_safe %>
    ]
  }
}
